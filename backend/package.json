{
  "name": "vite-project",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:server": "nodemon --watch 'server/**/*.ts' --exec 'ts-node' server/server.ts",
