    "sqlite3": "^5.1.7",
    "vue": "^3.4.15"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.10",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-vue": "^5.0.3",
    "nodemon": "^3.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.12",
    "vue-tsc": "^1.8.27"
  }
}
